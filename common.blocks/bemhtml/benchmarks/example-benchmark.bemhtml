block b-link-base {
  mix: [{mods: { key: 'value' }}]
}


block b-link {
    mix: [{ block: 'b-link-base' }]

    tag: 'a'

    attrs: {

        var ctx = this.ctx,
            a = { href: ctx.url },
            props = ['title', 'target'], p;

        while(p = props.pop()) ctx[p] && (a[p] = ctx[p]);

        return a;

    }

}

